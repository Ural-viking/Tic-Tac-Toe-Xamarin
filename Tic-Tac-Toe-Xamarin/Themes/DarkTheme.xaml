                    x:Class="Tic_Tac_Toe_Xamarin.Themes.DarkTheme">
    <Color x:Key="PageBackgroundColor">Black</Color>
    <Color x:Key="NavigationBarColor">#0EA1ED</Color>
    <Color x:Key="PrimaryColor">#E1C82E</Color>
    <Color x:Key="SecondaryColor">Gray</Color>
    <Color x:Key="PrimaryTextColor">#272B44</Color>
    <Color x:Key="SecondaryTextColor">#E1C82E</Color>
    <Color x:Key="TertiaryTextColor">WhiteSmoke</Color>
    <Color x:Key="TransparentColor">#E1C82E</Color>
</ResourceDictionary>